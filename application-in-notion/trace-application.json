        "inputSchema": "{\n  \"type\": \"object\",\n  \"properties\": {\n    \"email_type\": {\n      \"type\": \"string\",\n      \"description\": \"The classification of the email's purpose.\",\n      \"enum\": [\n        \"JOB_APPLICATION\",\n        \"NEWSLETTER\",\n        \"PROMOTION\",\n        \"PERSONAL\",\n        \"OTHER\"\n      ]\n    },\n    \"company_name\": {\n      \"type\": [\"string\", \"null\"],\n      \"description\": \"The full name of the company.\"\n    },\n    \"job_title\": {\n      \"type\": [\"string\", \"null\"],\n      \"description\": \"The title of the position applied for.\"\n    },\n    \"location\": {\n      \"type\": [\"string\", \"null\"],\n      \"description\": \"The city where the job is located.\"\n    },\n    \"application_date\": {\n      \"type\": [\"string\", \"null\"],\n      \"format\": \"date\",\n      \"description\": \"The date the email was received, formatted as YYYY-MM-DD.\"\n    },\n    \"status\": {\n      \"type\": [\"string\", \"null\"],\n      \"description\": \"The current stage of the application process.\",\n      \"enum\": [\n        \"Applied\",\n        \"Rejected\",\n        \"Interview\",\n        \"Under Review\",\n        null\n      ]\n    },\n    \"contact_email\": {\n      \"type\": [\"string\", \"null\"],\n      \"format\": \"email\",\n      \"description\": \"The sender's contact email address.\"\n    }\n  },\n  \"required\": [\n    \"email_type\"\n  ]\n}"
